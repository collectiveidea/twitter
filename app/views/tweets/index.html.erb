<%= form_for @tweet do |form| %>
  <p>
    <%= form.label :body %><br>
    <%= form.text_area :body, :rows => 3 %>
  </p>
  
  <p>
    <%= form.label :username %>
    @<%= form.text_field :username %>
  </p>
  
  <p>
    <%= form.submit 'Tweet' %>
  </p>
<% end %>

<%= render @tweets %>

<%= paginate @tweets %>